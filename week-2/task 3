s = input()

a, op, b, _, c = s

if a == 'x':
    if op == '+':
        print(int(c) - int(b))
    else:
        print(int(c) + int(b))

elif b == 'x':
    if op == '+':
        print(int(c) - int(a))
    else:
        print(int(a) - int(c))

else:
    if op == '+':
        print(int(a) + int(b))
    else:
        print(int(a) - int(b))